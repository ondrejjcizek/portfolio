{
    "useTabs": false,
    "singleQuote": true,
    "semi": false,
    "arrowParens": "avoid",
    "trailingComma": "all",
    "printWidth": 50,
    "tabWidth": 4,
    "plugins": ["prettier-plugin-svelte"],
    "pluginSearchDirs": ["."],
    "overrides": [{ "files": "*.svelte", "options": { "parser": "svelte" } }]
}
